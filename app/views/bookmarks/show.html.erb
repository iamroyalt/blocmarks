<h1><a href="<%= @bookmark.url %>"><%= @bookmark.name %></a></h1>
 <img src="<%= @bookmark.url %>"><class="img-thumbnail">
 <div class="row">
   <div class="col-md-4">
     <%= link_to "Edit", edit_topic_bookmark_path(@bookmark.topic, @bookmark), class: 'btn btn-success btn-xs' %>
     <%= link_to "Delete Bookmark", [@bookmark.topic, @bookmark], method: :delete, class: 'btn btn-danger btn-xs', data: {confirm: "Are you sure you want to delete this bookmark?"} %>
   </div>
 </div>
